{
  "manifest_version": 2,
  "name": "Budget Manager",
  "version": "1.0",
  "description": "This Chrome extension can help you keep track of your budget.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "budget.html"
  },
  "permissions": [
    "storage"
  ]
}
